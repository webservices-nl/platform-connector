{
  "name": "webservices-nl/platform-connector",
  "description": "platform connector generator for Webservices.nl",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-xml": "*",
    "php-http/client-implementation": "^1",
    "php-http/plugins": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "webservices-nl/common": "^0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4",
    "mockery/mockery": "^0.9",
    "monolog/monolog": "^1.18",
    "guzzlehttp/guzzle": "^6",
    "php-http/guzzle6-adapter": "^1.0",
    "league/factory-muffin": "^2.1",
    "league/factory-muffin-faker": "~1.0-beta",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  }
}
