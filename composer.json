{
  "name": "webservices-nl/platform-connector",
  "description": "platform connector generator for Webservices.nl",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-xml": "*",
    "ext-soap": "*",
    "guzzlehttp/guzzle": "^6",
    "psr/log": "^1.0",
    "webservices-nl/common": "^1",
    "webservices-nl/utils": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "mockery/mockery": "^0.9",
    "monolog/monolog": "^1.18",
    "league/factory-muffin": "^2",
    "friendsofphp/php-cs-fixer": "^2",
    "sensiolabs/security-checker": "^4"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.sup.ws.fdmg.org/"
    }
  ]

}
