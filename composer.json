{
  "name": "webservices-nl/platform-connector",
  "description": "platform connector generator for Webservices.nl",
  "keywords": [
    "soap",
    "soapclient",
    "webservicesNl",
    "webservices.nl"
  ],
  "config": {
    "gitlab-domains": [
      "gitlab.fdmg.org"
    ],
    "preferred-install": "dist"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WebservicesNL",
      "email": "tech@webservices.nl",
      "homepage": "http://www.webservices.nl"
    }
  ],
  "require": {
    "php": "^5.6 || ^7",
    "ext-xml": "*",
    "guzzlehttp/guzzle": "^6",
    "psr/log": "^1.0",
    "webservices-nl/common": "^0",
    "webservices-nl/utils": "1.0.4",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4",
    "mockery/mockery": "^0.9",
    "monolog/monolog": "^1.18",
    "league/factory-muffin": "^2",
    "squizlabs/php_codesniffer": "^2",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "WebservicesNl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebservicesNl\\Test\\": "test/"
    }
  }
}
